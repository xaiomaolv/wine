  font-size: 28rpx !important;
  color: #484848 !important;
  position: relative;
}

.movable-view {
  pointer-events: auto;
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  background: var(--primary-color);
  color: #FFF;
  border-radius: 120rpx;
  text-align: center;
  font-size: 58rpx;
  position: relative;
}

.movable-view .icon {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.lodemore {
  display: inline-block;
  margin-top: 40rpx;
  color: var(--primary-color);
  cursor: pointer;
  letter-spacing: 3px;
  font-weight: 600;
  line-height: 56rpx;
  position: relative;
  padding-left: 60rpx;
}

