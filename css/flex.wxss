/*滚动条样式*/
.scroll {
  height: 100%;
  overflow-y: auto;
  padding: 30rpx 20rpx;
}

.scroll::-webkit-scrollbar {
  width: 4px;
  /*height: 4px;*/
}

.scroll::-webkit-scrollbar-thumb {
  border-radius: 10px;
  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
  background: rgba(0, 0, 0, 0.2);
}

.scroll::-webkit-scrollbar-track {
  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
  border-radius: 0;
  background: rgba(0, 0, 0, 0.1);
}

.container {
  min-height: 100%;
