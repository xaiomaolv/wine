/* 左箭头：border-width: 0 0 2px 2px; */
.arrow-left {
  /* position: absolute; */
  right: 50rpx;
  top: 150rpx;
  content: "";
  display: inline-block;
  height: 16rpx;
  width: 16rpx;
  border-width: 0 0 2px 2px;
  border-color: rgb(26, 25, 25);
  border-style: solid;
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
}

